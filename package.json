{
  "name": "protal-cms",
  "version": "0.1.0",
  "description": "protal cms",
  "keywords": [
    "react",
    "router",
    "redux",
    "immutable",
    "webpack"
  ],
  "scripts": {
    "clean": "rimraf ./public/dist",
    "eslint": "eslint client/** ",
    "eslintFix": "eslint client/** --fix",
    "dll": "webpack --progress --config webpack.config.dll.babel.js",
    "generateEnv": "node setEnv.js",
    "prebuildCode": "npm run clean && npm run generateEnv",
    "buildCode": "webpack -p --progress --colors --config webpack.build.babel.js",
    "build": "cross-env NODE_ENV=production npm run buildCode && sh ./toDist.sh",
    "buildTemplate": "cross-env NODE_ENV=development npm run buildCode && npm run clean",
    "prestart": "cross-env npm run clean && cross-env NODE_ENV=development npm run generateEnv && npm run buildTemplate",
    "start": "nodemon ./bin/www",
    "startDev": "cross-env NODE_ENV=development nodemon ./bin/www",
    "startProd": "cross-env NODE_ENV=production node ./bin/www"
  },
